<!DOCTYPE HTML>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Start your development with a Dashboard for Bootstrap 4.">
    <meta name="author" content="Creative Tim">
    <title>{{title}}</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="/img/brand/fabiconvia.ico" data-head-react="true">

    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@mdi/font@6.6.96/css/materialdesignicons.min.css">
    
    <!-- Icons -->
    <link rel="stylesheet" href="/vendor/nucleo/css/nucleo.css" type="text/css">
    <link rel="stylesheet" href="/vendor/@fortawesome/fontawesome-free/css/all.min.css" type="text/css">
    <!-- Argon CSS -->
    <link rel="stylesheet" href="/css/argon.min.css" type="text/css">
    <link rel="stylesheet" href="/css/app.min.css">

    {{#ifCond page 'disp'}}
        <link rel="stylesheet" href="/css/aguarde_css.css" type="text/css">
        <link rel="stylesheet" href="/css/style2.css">
        <link rel="stylesheet" href="/css/styne-stn.css">
    {{/ifCond}}
    <!-- Data Tables -->
    <link rel="stylesheet" href="/vendor/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="/vendor/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css">
    <link rel="stylesheet" href="/vendor/datatables.net-select-bs4/css/select.bootstrap4.min.css">
    <link rel="stylesheet" href="/vendor/animate.css/animate.min.css">
    <link rel="stylesheet" href="/vendor/sweetalert2/dist/sweetalert2.min.css">
    <link rel="stylesheet" href="/css/styles.css">
</head>

<body class="{{pageClasses}} {{#contains pageType 'site'}}page{{/contains}}" data-token="{{token}}" data-client="{{client.id}}" id="{{pageId}}">
    {{{body}}}
    <!-- Argon Scripts -->
    <script src="/socket.io/socket.io.js"></script>
    <script>
        //var socket = io();

        var socket = io.connect(`/`, {
            transports: ['websocket'],
            secure: true,
            'force new connection' : true,
            'reconnect' : true,
            reconnection: true,
            reconnectionDelay: 1000,
            reconnectionDelayMax : 5000,
            reconnectionAttempts: 99999
        })
    </script>
    <!-- Core -->
    <script src="/vendor/jquery/dist/jquery.min.js"></script>
    <script src="/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/vendor/js-cookie/js.cookie.js"></script>
    <script src="/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
    <script src="/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
    <!-- Data Tables -->
    <script src="/vendor/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="/vendor/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="/vendor/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="/vendor/datatables.net-buttons-bs4/js/buttons.bootstrap4.min.js"></script>
    <script src="/vendor/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="/vendor/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="/vendor/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="/vendor/datatables.net-select/js/dataTables.select.min.js"></script>
    <script src="/vendor/list.js/dist/list.min.js"></script>
    <!-- Money Mask -->
    <script src="/js/jquery.maskMoney.min.js"></script>
    <!-- Alerts -->
    <script src="/vendor/bootstrap-notify/bootstrap-notify.min.js"></script>
    <!-- TAGS // -->
    <script src="/vendor/bootstrap-tagsinput/dist/bootstrap-tagsinput.min.js"></script>
    <!-- Argon JS -->
    {{#ifCond page 'dashboard'}}
    <!-- Optional JS -->
    <script src="/vendor/chart.js/dist/Chart.min.js"></script>
    <script src="/vendor/chart.js/dist/Chart.extension.js"></script>
    {{/ifCond}}

    <script src="/vendor/sweetalert2/dist/sweetalert2.min.js"></script>
    <script src="/js/cleave.min.js"></script>
    <script src="/js/addons/cleave-phone.br.js"></script>
    <script src="/js/argon.min.js?v=1.2.0"></script>
    <script src="/js/scripts.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    {{#if await}}
    <script src="/js/await.js"></script>
    {{/if}}

    {{#if panel}}
    <script src="/js/panel.js"></script>
    {{/if}}

    {{#if operator}}
    <script src="/js/operator.js"></script>
    {{/if}}
    {{>notify}}
</body>

</html>